{
    "name": "dameter/app",
    "description": "dameter",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2.0",
        "ext-json": "*",
        "yiisoft/yii2": "~2.0.45",
        "yiisoft/yii2-bootstrap5": "~2.0.2",
        "yiisoft/yii2-symfonymailer": "~2.0.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8",
        "yiisoft/yii2-debug": "^2.1"

    },
    "autoload": {
        "psr-4": {
            "dameter\\app\\": "src"
        }
    },
    "authors": [
        {
            "name": "tonis",
            "email": "tonis@andmemasin.eu"
        }
    ],
    "config": {
        "platform": {
            "php": "8.2.0"
        },
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
